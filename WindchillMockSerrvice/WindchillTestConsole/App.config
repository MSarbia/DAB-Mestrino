<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="ExtPortBinding" maxReceivedMessageSize="2097152"
                         maxBufferSize="2097152"
                         maxBufferPoolSize="2097152" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:62992/WindchillDocService.asmx"
          binding="basicHttpBinding" bindingConfiguration="ExtPortBinding"
          contract="WindchillDocService.Ext" name="ExtPort" />
    </client>
  </system.serviceModel>
</configuration>